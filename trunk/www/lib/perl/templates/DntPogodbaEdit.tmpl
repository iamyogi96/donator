function Racunaj(){
    
    var celoten= document.getElementById('edb_celoten');
    var prvi= document.getElementById('edb_amount1');
    var st= document.getElementById('edb_num_installments');
    var ostali= document.getElementById('edb_amount2');
    var rezultat;
    
    celoten= celoten.value.replace(',','.');
    celoten=parseFloat(celoten);
    if(prvi.value.length < 1){
        prvi.value = "0,00";
    }
    prvi= prvi.value.replace(',','.');
    prvi=parseFloat(prvi);
    
    st= parseInt(st.value);
    
    if(prvi>celoten){
        prvi.value="0,00";
    }
    
    rezultat= (celoten - prvi)/st;
    
    rezultat= Math.round(rezultat*100)/100;
    
    rezultat= rezultat.toString();
    rezultat= rezultat.replace('.',',');
    
    if(rezultat=="NaN"){
        rezultat="";
    }
    else if(rezultat.indexOf(',')==-1){
        rezultat=rezultat+",00";
    }
    else if(rezultat.indexOf(',')-rezultat.length==-2){
        rezultat=rezultat+"0";
    }   

    ostali.value= rezultat;
    
    Uredi();
}
