function Uredi()
{
    var form = document.myForm;   

    NastaviDirektne();
    NastaviStatus(); 
    NastaviZneske();
    
    var count=0;
    for(i = 0; i < form.length; i++){        

        var disable = false;
        var require = "";
        
        if(required[form[i].id] == 1){
            require = "required";
        }
        if(disabled[form[i].id] == 1){
            disable = true;
        }
          
        labels = form.getElementsByTagName('label');
        for(j = 0; j < labels.length; j++){
            if(labels[j].htmlFor == form[i].id){
                labels[j].className = require;                
            }
        }
        
        if(form[i].type == "text" || form[i].tagName == "TEXTAREA"){
            
            form[i].readOnly = disable;
            if(disable && form[i].id != "edb_id_agreement" && form[i].id != "edb_id_donator"
               && form[i].id != "edb_postnaSt2"){
                form[i].value = "";
                form[i].style.border = "";
            }
            //form[i].disabled = disable;
        }
        else{
            
            form[i].disabled = disable;
            if(form[i].id == "edb_nacinPlacila" || form[i].id == "edb_frekvenca"){
                if(disable){
                    form[i].options[0].selected = true;
                }
            }
        }
            
        

    }
}


